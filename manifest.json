{
  "manifest_version": 2,
  "name": "Proxy Cat",
  "version": "0.1.0",
  "description": "Assign proxies per site, tab, or tab group.",
  "permissions": [
    "proxy",
    "tabs",
    "contextMenus",
    "storage",
    "<all_urls>"
  ],
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "91.1.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
  "background": {
    "scripts": ["storage.js", "matcher.js", "background.js"]
  },
  "browser_action": {
    "default_title": "Proxy Cat",
    "default_popup": "popup/popup.html"
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "icons": {
    "128": "icons/icon.png"
  }
}
